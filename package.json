{
  "name": "blockchain-interesting-bot",
  "version": "1.0.0",
  "description": "Some interesting blockchain bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env tsc --project tsconfig.json",
    "prestart": "npm run build",
    "start": "cross-env node -r dotenv/config ./dist/index.js dotenv_config_path=./.env",
    "start:prod": "node ./dist/index.js",
    "lint": "cross-env eslint --ext .ts src --quiet",
    "lint:fix": "cross-env eslint --ext .ts src --quiet --fix",
    "format": "cross-env prettier --check src",
    "format:fix": "cross-env prettier --write src",
    "check": "npm run format && npm run lint",
    "check:fix": "npm run format:fix && npm run lint:fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "chalk": "^5.0.0",
    "colors.ts": "^1.0.20",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^17.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymnasy55/blockchain-interesting-bot.git"
  },
  "author": "gymnasy55, kostyamospan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gymnasy55/blockchain-interesting-bot/issues"
  },
  "homepage": "https://github.com/gymnasy55/blockchain-interesting-bot#readme"
}
